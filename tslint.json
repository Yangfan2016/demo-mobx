{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "semicolon": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": false,
        "import-name": false,
        "indent": [
            true,
            "spaces",
            4
        ]
    },
    "rulesDirectory": []
}